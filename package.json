    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-react-hooks": "^4.1.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5"
